{
  "name": "storybook-addon-code-preview",
  "version": "0.0.1",
  "description": "A storybook addon for previewing component files",
  "keywords": [
    "storybook-addons",
    "style",
    "test",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alex-streza/code-preview"
  },
  "author": {
    "name": "Alex Streza",
    "email": "alex.streza@okapistudio.com"
  },
  "license": "MIT",
  "main": "dist/preset.js",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "babel ./src --out-dir ./dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "start": "concurrently \"npm run clean\" \"npm run storybook -- --no-manager-cache --quiet\" \"npm run build -- --watch\"",
    "build-storybook": "build-storybook",
    "release": "npm run build && auto shipit"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Addon Preview Code",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@storybook/addons": "^6.1.21",
    "@storybook/api": "^6.1.21",
    "@storybook/components": "^6.1.21",
    "@storybook/core-events": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@storybook/theming": "^6.1.21",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-select": "^4.2.1",
    "react-syntax-highlighter": "^15.4.3"
  },
  "devDependencies": {
    "auto": "^10.3.0",
    "concurrently": "^5.3.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
